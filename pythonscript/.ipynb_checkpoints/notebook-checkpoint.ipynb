   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "np.set_printoptions(precision=3, suppress=True)\n",
    "\n",
    "x = np.linspace(0.01, 0.99, 990)\n",
    "\n",
    "y = 3*np.log(0.5*(1-x)*2)*2+1*np.log(0.5*x*2)\n",
    "\n",
    "plt.figure(1)\n",
    "plt.plot(x,y)\n",
    "plt.show()\n",
    "print(x[np.argmax(y)])\n"
   ]
